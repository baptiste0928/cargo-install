{
  "name": "cargo-install",
  "version": "3.0.0",
  "description": "GitHub action for cache-efficient Rust crates installation",
  "author": "baptiste0928",
  "license": "ISC",
  "scripts": {
    "check": "tsc",
    "lint": "ts-standard src",
    "fix": "ts-standard src --fix",
    "package": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js"
  },
  "dependencies": {
    "@actions/cache": "^3.2.4",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.2.1",
    "@actions/io": "^1.1.3",
    "chalk": "^5.3.0",
    "semver": "^7.6.0",
    "string-argv": "^0.3.2",
    "valibot": "^0.30.0"
  },
  "devDependencies": {
    "@types/semver": "^7.5.8",
    "esbuild": "^0.20.1",
    "ts-standard": "^12.0.2",
    "typescript": "^5.4.2"
  }
}
