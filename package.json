{
  "name": "cargo-install",
  "version": "3.3.1",
  "description": "GitHub action for cache-efficient Rust crates installation",
  "author": "baptiste0928",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "scripts": {
    "check": "tsc",
    "lint": "eslint src",
    "format": "prettier . !pnpm-lock.yaml !dist --write",
    "format:check": "prettier . !pnpm-lock.yaml !dist --check",
    "package": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js"
  },
  "dependencies": {
    "@actions/cache": "^4.0.5",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.2.3",
    "@actions/io": "^1.1.3",
    "chalk": "^5.6.0",
    "semver": "^7.7.2",
    "string-argv": "^0.3.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "@types/semver": "^7.7.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
