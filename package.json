{
  "name": "cargo-install",
  "version": "3.4.0",
  "description": "GitHub action for cache-efficient Rust crates installation",
  "author": "baptiste0928",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "scripts": {
    "check": "tsc",
    "lint": "oxlint --type-aware",
    "format": "prettier . !pnpm-lock.yaml !dist --write",
    "format:check": "prettier . !pnpm-lock.yaml !dist --check",
    "package": "esbuild src/index.ts --bundle --platform=node --target=node24 --outfile=dist/index.js"
  },
  "dependencies": {
    "@actions/cache": "^6.0.0",
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@actions/http-client": "^4.0.0",
    "@actions/io": "^3.0.2",
    "chalk": "^5.6.2",
    "semver": "^7.7.4",
    "string-argv": "^0.3.2",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.13",
    "@types/semver": "^7.7.1",
    "esbuild": "^0.27.3",
    "oxlint": "^1.49.0",
    "oxlint-tsgolint": "^0.14.2",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
