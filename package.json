{
  "name": "cargo-install",
  "version": "3.3.0",
  "description": "GitHub action for cache-efficient Rust crates installation",
  "author": "baptiste0928",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "scripts": {
    "check": "tsc",
    "lint": "eslint src",
    "format": "prettier . !pnpm-lock.yaml !dist --write",
    "format:check": "prettier . !pnpm-lock.yaml !dist --check",
    "package": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js"
  },
  "dependencies": {
    "@actions/cache": "^4.0.3",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.2.3",
    "@actions/io": "^1.1.3",
    "chalk": "^5.4.1",
    "semver": "^7.7.2",
    "string-argv": "^0.3.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^20.19.1",
    "@types/semver": "^7.7.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
