{
  "name": "cargo-install",
  "version": "3.3.0",
  "description": "GitHub action for cache-efficient Rust crates installation",
  "author": "baptiste0928",
  "license": "ISC",
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "check": "tsc",
    "lint": "eslint src",
    "format": "prettier . !pnpm-lock.yaml !dist --write",
    "format:check": "prettier . !pnpm-lock.yaml !dist --check",
    "package": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js"
  },
  "dependencies": {
    "@actions/cache": "^4.0.0",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.2.3",
    "@actions/io": "^1.1.3",
    "chalk": "^5.4.1",
    "semver": "^7.6.3",
    "string-argv": "^0.3.2",
    "valibot": "^0.33.3"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "@types/semver": "^7.5.8",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
